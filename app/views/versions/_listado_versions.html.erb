<% if @versions.blank?  %>
    No hay versiones encontradas...
<%else%>
    <table>
      <tr>
        <th>Gemset</th>
        <th>Version</th>
        <th>Revision</th>
        <th></th>
        <th></th>
        <th></th>
      </tr>

      <% @versions.each do |version| %>
          <tr>
            <td><%= version.gemset.nombre %></td>
            <td><%= version.numero_version %></td>
            <td><%= version.revision %></td>
            <td><%= link_to 'Show', version %></td>
            <td><%= link_to 'Edit', edit_version_path(version) %></td>
            <td><%= link_to 'Destroy', version, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
      <% end %>
    </table>
<% end %>
